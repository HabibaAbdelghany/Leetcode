class Solution {
public:
    string longestCommonPrefix(vector<string>& str) {
       string ans="";
        for(int i=0; i<str[0].size();i++)
        {

            char x=str[0][i];
            for(int j =0; j<str.size();j++)
            {
 if(str[j][i]!=x)
     return ans;
            }
         
ans+=x;
            }
  return ans;
    }
};
